<data>
    <xpath
        expr="/form/notebook/page[@id=&quot;general&quot;]"
        position="after">
            <page string="Relation" id="relation">
                <label name="sale"/>
                <field name="sale"/>
            </page>
    </xpath>
    <xpath
        expr="/form/notebook/page[@id=&quot;stock&quot;]/field[@name=&quot;stock_moves&quot;]"
        position="after">
            <label name="shipment_info"/>
            <field name="shipment_info"/>
            <field name="shipments" colspan="4"/>
            <field name="shipment_returns" colspan="4"/>
    </xpath>
</data>
